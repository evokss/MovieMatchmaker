<h2 class="page-header">Search Movie Reviews</h2>
<form action="/movies" method="GET">
  <div>
    <label>Movie Title</label>
    <input
      type="text"
      name="movieTitle"
      value="<%= searchOptions.movieTitle %>"
    />
  </div>

  <button type="submit">Search</button>
</form>

<% movies.forEach(movie => { %>
<a href="/movies/<%= movie.id %>">
  <img height="150" width="100" src="<%= movie.posterImagePath %>" />
</a>
<% }) %>
